{
  "version": 2,
  "builds": [
    {"src": "/api/index.php", "use": "vercel-php@0.5.2"},
    {"src": "/api/**", "use": "vercel/static"}
  ],

  "routes": [
    {
      "src": "/css/(.*)", "dest": "/css/$1"
    },
    {
      "src": "/img/(.*)", "dest": "/img/$1"
    },
    {
      "src": "/(.*)", "dest": "/api/index.php"
    }
  ]
}